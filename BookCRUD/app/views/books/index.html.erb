<% if user_signed_in? %>
<h1>Books List</h1>
<table>
    <tr>
    <th>Book Title</th>
    <th>Author_Name</th>
    <th>Description</th>
    <th>User ID</th>
    </tr>
    <% @books.each do |book| %>
    <tr>
        <td><%= link_to book.title, book%> </td>
        <td><%= book.author_name%></td>
        <td><%= book.description%> </td>
        <td> <% if book.user_id == 2 %>
            Admin_entry
          <% else %>
            User_entry
          <% end %>
          </td>
    </tr>
      <% end %>
</table>
<p><%= link_to "New Book", new_book_path %></p>
<% end %>
